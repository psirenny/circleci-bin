  "license": "Unlicense",
  "main": "dist/index.js",
  "repository": "github:psirenny/circleci-bin",
  "scripts": {
    "audit": "yarn audit",
    "build": "rollup -c",
    "check": "run-s lint test test:cov typecheck",
    "ci:check": "ci:validate ci:run",
    "ci:run": ".bin/circleci build --job check",
    "ci:validate": ".bin/circleci config validate",
    "clean": "trash **.DS_Store *.log .bin .nyc_output coverage dist node_modules",
    "flow:install": "run-s flow:update",
    "flow:stub": "flow-typed create-stub",
    "flow:update": "flow-typed update",
    "install:bin": "dist/install-binary",
    "install:dev": "yarn install --ignore-scripts && yarn run build && yarn install --force",
    "lint": "run-s lint:js lint:md lint:pkg",
    "lint:js": "eslint . --ext .js,.md",
    "lint:md": "remark --frail .",
    "lint:pkg": "npmPkgJsonLint ./package.json",
    "outdated": "yarn outdated",
    "prepublish": "run-s build",
    "publish": "standard-version",
    "release": "git push --follow-tags origin master && yarn publish",
    "test": "nyc ava --verbose",
    "test:cov": "nyc check-coverage",
    "test:cov:report": "nyc report --reporter=html",
    "typecheck": "flow .",
    "update": "run-s update:bin update:js update:yarn",
    "update:bin": "babel-node src/code/scripts/update-version",
    "update:js": "yarn upgrade-interactive --latest",
    "update:yarn": "yarn policies set-version"
  },
  "sideEffects": false
}
